func print_int(x);
func input_int();

func sum(x, y) {
	return x - y;
}

func adder(x) {
	return \(y) -> sum(x, y);
}

func main() {
	x <- input_int();
	y <- input_int();
	f <- adder(x);
	s <- f(y);

	if ( (s==0) ) {
		print_int(1);
	} else {
		print_int(2);
	}

	if ( (s==0) || (s == (0-1)) ) {
		print_int(1);
	} else {
		print_int(2);
	}
}
